        "@babel/eslint-parser": "^7.18.9",
        "@commitlint/cli": "^17.0.3",
        "@commitlint/config-conventional": "^17.0.3",
        "commander": "^9.4.0",
        "eslint": "^8.22.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "gray-matter": "^4.0.3",
        "husky": "^8.0.1",
        "js-yaml": "^4.1.0",
        "lint-staged": "^13.0.3",
        "markdownlint": "^0.26.2",
        "markdownlint-cli2": "^0.5.1",
        "prettier": "2.7.1"
    }
